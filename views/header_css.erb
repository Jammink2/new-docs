div#header {
  background-color: #333333;
  color: #BBBBBB;
  height: 50px;
  padding: 20px;
}

div#header li {
  list-style:none;
}

#td-header-inner { width: 1140px; }
#td-logo { float: left; margin-top: -32px;}
#td-logo a {
  display: block;
  margin: 0 auto;
  width: 379px;
  height: 60px;
  text-indent: -9999px;
  background: url("http://www.treasure-data.com/images/brand_side.png") no-repeat 28px top;
}

#td-nav { margin-top: 32px; }
#td-nav ol, #td-nav ul { padding: 0; }
#td-nav ol li, #td-nav ul li { margin: 0; }
#td-nav li { font-weight: bold; font-size: 14px; text-shadow: 0px 1px 0px #111; }
#td-nav li a { color: #bbb; text-decoration: none; }
#td-nav li a:hover { color: #eee; }
#td-nav li a.td-signup {
  float: left;
  padding: 5px 15px;
  margin-top: -4px;
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  border: 1px solid #333;
  font: 14px/21px "Helvetica Neue", Arial, sans-serif;
  font-weight: bold;
  color: #fff;
  cursor: pointer;
  text-decoration: none;
  -webkit-box-shadow: inset 0px 0px 1px rgba(255,255,255,0.5);
  -moz-box-shadow: inset 0px 0px 1px rgba(255,255,255,0.5);
  box-shadow: inset 0px 0px 1px rgba(255,255,255,0.5);
  background-image: -webkit-linear-gradient(top, #7fab59, #537e2e);
  background-image: -moz-linear-gradient(top, #7fab59, #537e2e);
  background-image: -linear-gradient(top, #7fab59, #537e2e);
  text-shadow: 0px -1px 0px rgba(0,0,0,0.6);
}
#td-nav li a.td-signup:hover {
  cursor: pointer;
  border: 1px solid #111;
}
#td-nav li a.td-signup:active {
  cursor: pointer;
  background: #537e2e;
}
